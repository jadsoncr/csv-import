{
  "name": "csv-import-react",
  "version": "1.0.18",
  "description": "Open-source CSV, TSV, and XLS/XLSX file importer for React and JavaScript",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tableflowhq/csv-import.git"
  },
  "keywords": [
    "csv",
    "import",
    "excel",
    "data",
    "importer",
    "react",
    "tableflow",
    "csv-import"
